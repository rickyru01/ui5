{
	"_version": "1.7.0",
	"sap.app": {
		"id": "demo1",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {

			"my_global_json_model_data_source": {
				"uri": "data/column_chart_data.json",
				"type": "JSON"
			}

		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ricky.test.ui5.demo1.view.App",
			"type": "XML",
			"id": "app",
			"async": true
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ricky.test.ui5.demo1.i18n.i18n"
				}
			},

			"my_global_json_model": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "my_global_json_model_data_source"
			}

		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"config": {
			"fullWidth": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "ricky.test.ui5.demo1.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"async": true,
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [{
					"pattern": "",
					"name": "appHome",
					"target": "home"
				},
				{
					"pattern": "flexLayout",
					"name": "flexLayout",
					"target": "flexLayout"
				},
				{
					"pattern": "goToMaster",
					"name": "master",
					"target": [
						"master",
						"detail"
					]
				},
				{
					"pattern": "detail/{product}/{layout}",
					"name": "detail",
					"target": [
						"master",
						"detail"
					]
				},
				{
					"pattern": "statusIndicator",
					"name": "statusIndicator",
					"target": "statusIndicator"
				},{
					"pattern": "dynamicPage",
					"name": "dynamicPage",
					"target": "dynamicPage"
				},{
					"pattern": "stepInput",
					"name": "stepInput",
					"target": "stepInput"
				},{
					"pattern": "microChartsInTable",
					"name": "microChartsInTable",
					"target": "microChartsInTable"
				},{
					"pattern": "pieChart",
					"name": "pieChart",
					"target": "pieChart"
				},{
					"pattern": "filterBar",
					"name": "filterBar",
					"target": "filterBar"
				}
			],
			"targets": {
				"home": {
					"viewId": "home",
					"viewName": "Home",
					"viewLevel": 1
				},
				"notFound": {
					"viewId": "notFound",
					"viewName": "NotFound",
					"transition": "show"
				},
				"flexLayout": {
					"viewId": "flexLayout",
					"viewPath": "ricky.test.ui5.demo1.view.flex_layout",
					"viewName": "2ColLayout",
					"viewLevel": 2
				},
				"master": {
					"viewName": "Master",
					"controlAggregation": "beginColumnPages"
				},
				"detail": {
					"viewName": "Detail",
					"controlAggregation": "midColumnPages"
				},
				"statusIndicator": {
					"viewId": "statusIndicator",
					"viewPath": "ricky.test.ui5.demo1.view",
					"viewName": "StatusIndicator",
					"viewLevel": 2
				},
				"dynamicPage": {
					"viewId": "dynamicPage",
					"viewPath": "ricky.test.ui5.demo1.view",
					"viewName": "DynamicPageAnalyticalTable",
					"viewLevel": 2
				},
				"stepInput": {
					"viewId": "stepInput",
					"viewPath": "ricky.test.ui5.demo1.view",
					"viewName": "StepInput",
					"viewLevel": 1
				},
				"microChartsInTable": {
					"viewId": "microChartsInTable",
					"viewPath": "ricky.test.ui5.demo1.view",
					"viewName": "MicroChartsInTable",
					"viewLevel": 1
				},
				"pieChart":{
					"viewId":  "pieChart",
					"viewPath":"ricky.test.ui5.demo1.view",
					"viewName": "PieChart",
					"viewLevel": 1
				},
				"filterBar":{
					"viewId":  "filterBar",
					"viewPath":"ricky.test.ui5.demo1.view",
					"viewName": "FilterBar",
					"viewLevel": 1
				}
				
				
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	}
}