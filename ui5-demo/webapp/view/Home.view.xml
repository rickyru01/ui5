<mvc:View
	controllerName="ricky.test.ui5.demo1.controller.Home"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page title="Home page(Index)" class="sapUiResponsiveContentPadding">
		<content>
			<Button text="Flex Layout (2 Column)" press=".onFlexLayout" class="sapUiTinyMarginEnd"/>
			<Button text="Status Indicator"  press=".onStatusIndicator" class="sapUiTinyMarginEnd"/>
			<Button id="displayNotFoundBtn" text="DisplayNotFound" press=".onDisplayNotFound" class="sapUiTinyMarginEnd"/>
			<Button text="TestDynamicPageWithPopOver" press=".onDynamicPage" class="sapUiTinyMarginEnd"/>
			<Button text="TestStepInput" press=".onStepInput" class="sapUiTinyMarginEnd"/>
			<Button text="MicroChartsInTable" press=".onMicroChartsInTable" class="sapUiTinyMarginEnd"/>
			<Button text="PieChart(Viz,DataModelTest)" press=".onTestPieChart" class="sapUiTinyMarginEnd"/>
			<Button text="FilterBar" press=".onTestFilterBar" tooltip="Search with busy dialog" class="sapUiTinyMarginEnd"/>
			<Button text="LiveSearch" press=".onLiveSearch" tooltip="Search live" class="sapUiTinyMarginEnd"/>
		</content>
	</Page>
</mvc:View>